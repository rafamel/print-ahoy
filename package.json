{
  "name": "print-ahoy",
  "version": "0.0.1",
  "author": "Rafa Mel",
  "description": "Print every PDF that comes into that one folder.",
  "main": "src/app.js",
  "scripts": {
    "start": "electron ./",
    "lint": "eslint ./src --ext .js",
    "lint:md": "markdownlint *.md --config markdown.json",
