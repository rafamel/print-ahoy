    "update:packages": "rm -rf node_modules && rm package-lock.json && npm update --save/save-dev && npm install && npm outdated"
  },
  "contributors": [
    {
      "name": "Rafa Mel"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/rafamel/print-ahoy"
  },
  "license": "MIT",
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/rafamel/print-ahoy/raw/master/LICENSE"
    }
  ],
  "bugs": {
    "url": "https://github.com/rafamel/-moduleName-/issues"
  },
  "dependencies": {
    "bluebird": "^3.5.1",
    "chokidar": "^1.7.0",
    "electron-pdf-window": "^1.0.12",
    "electron-platform": "^1.2.0",
    "electron-settings": "^3.1.3",
    "fs-extra": "^4.0.2",
    "moment": "^2.19.1",
    "nedb": "^1.8.0",
    "sleep-promise": "^2.0.0"
  },
  "devDependencies": {
    "electron-cli": "^0.2.8",
    "eslint": "^4.9.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "markdownlint-cli": "^0.3.1",
    "npm-run-all": "^4.1.1"
  },
  "engines": {
    "node": ">=8.0.0"
  }
}
